{
  "manifest_version": 3,
  "name": "Keymark",
  "description": "Keyboard-first bookmark launcher",
  "version": "1.0.0",
  "permissions": ["bookmarks", "tabs"],
  "action": {
    "default_popup": "index.html"
  },
  "background": {
  "service_worker": "background.js"
},
"commands": {
  "open-keymark": {
    "suggested_key": {
      "default": "Ctrl+K",
      "mac": "Command+K"
    },
    "description": "Open Keymark"
  }
}
}
